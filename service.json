{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "$SERVICE_CONTAINER",
        "ports": [ 
          { "HostIP": "0.0.0.0", "HostPort": "80:80/tcp" }
	]
      }
    }
  }
}

